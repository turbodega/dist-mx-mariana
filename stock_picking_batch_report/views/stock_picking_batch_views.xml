<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="stock_picking_batch_form_turbodega" model="ir.ui.view">
        <field name="name">stock.picking.batch.turbodega.form</field>
        <field name="model">stock.picking.batch</field>
        <field name="inherit_id" ref="stock_picking_batch.stock_picking_batch_form" />
        <field name="arch" type="xml">
            <xpath expr="//header/button[@name='action_print']" position="after">
                <button
                    name="action_print_group"
                    string="Print by product"
                    type="object"
                    states="done"
                />
            </xpath>
        </field>
    </record>


</odoo>
